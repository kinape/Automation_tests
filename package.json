{
  "name": "automation_tests",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "api:start": "node api/server.js",
    "report": "node generate-report.js",
    "cy:run": "npm run test && npm run report",
    "k6:run": "k6 run k6/load-test.js",
    "test:mobile": "set \"NODE_OPTIONS=\" && wdio run ./mobile/config/wdio.conf.js --autoCompileOpts.autoCompile=false",
    "test:mobile:ci": "NODE_OPTIONS= wdio run ./mobile/config/wdio.conf.js --autoCompileOpts.autoCompile=false",
    "test:mobile:win": "set \"NODE_OPTIONS=\" && if not defined ANDROID_HOME set \"ANDROID_HOME=%LOCALAPPDATA%\\Android\\Sdk\" && if not defined ANDROID_SDK_ROOT set \"ANDROID_SDK_ROOT=%ANDROID_HOME%\" && set \"PATH=%PATH%;%ANDROID_HOME%\\platform-tools;%ANDROID_HOME%\\emulator;%ANDROID_HOME%\\tools;%ANDROID_HOME%\\tools\\bin\" && wdio run ./mobile/config/wdio.conf.js --autoCompileOpts.autoCompile=false",
    "android:emu:start": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\mobile\\scripts\\start-avd.ps1 -AvdName Pixel_6_API_34",
    "android:emu:start:headless": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\mobile\\scripts\\start-avd.ps1 -AvdName Pixel_6_API_34 -GpuMode swiftshader_indirect -NoWindow",
    "test:mobile:with-avd": "npm run android:emu:start:headless && set \"AVD_NAME=Pixel_6_API_34\" && npm run test:mobile:win",
    "allure:generate:mobile": "npx allure generate --clean mobile/reports/allure-results -o mobile/reports/allure-report",
    "allure:open:mobile": "npx allure open mobile/reports/allure-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kinape/Automation_tests.git"
  },
  "keywords": [],
  "author": "Rogerio Melo Kinape",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kinape/Automation_tests/issues"
  },
  "homepage": "https://github.com/kinape/Automation_tests#readme",
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@wdio/cli": "^8.38.2",
    "@wdio/local-runner": "^8.38.2",
    "@wdio/mocha-framework": "^8.38.0",
    "@wdio/spec-reporter": "^8.38.2",
    "@wdio/allure-reporter": "^8.38.2",
    "allure-commandline": "^2.27.0",
    "@wdio/appium-service": "^8.38.2",
    "appium": "^2.10.3",
    "appium-uiautomator2-driver": "^3.5.3",
    "cypress": "^15.5.0",
    "esbuild-plugins-node-modules-polyfill": "^1.7.1",
    "multiple-cucumber-html-reporter": "^3.9.3",
    "webdriverio": "^8.38.2",
    "@badeball/cypress-cucumber-preprocessor": "^23.2.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.6",
    "express": "^4.19.2"
  },
  "dependencies": {},
  "overrides": {
    "@cucumber/messages": "^30.1.0"
  }
}
